pipeline {
    agent any
    stages {
        stage ('Initialize') {
            steps {
                echo  "Initializing the Code File"
            }
        }
 
        stage ('Build') {
            steps {
                echo 'Hello World how are you!!!!'
            }
        }
 
         stage ('Deploy') {
            steps {
                echo 'Deployed an Artifact'
            }
        }
    }
}
